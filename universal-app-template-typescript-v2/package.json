{
  "name": "spirokit-universal-app-template-typescript-v2",
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [
    "react",
    "expo",
    "template",
    "typescript",
    "spirokit"
  ],
  "description": "Base universal app template for using Expo + NextJS + Solito and SpiroKit",
  "license": "MIT",
  "scripts": {
    "native": "cd apps/expo && expo start",
    "web": "cd apps/next && yarn next"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.69.5",
    "eslint": "^8.21.0",
    "turbo": "^1.4.2",
    "typescript": "^4.7.4"
  },
  "private": true
}
